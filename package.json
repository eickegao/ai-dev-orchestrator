    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.13.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.1.2",
    "electron": "^31.4.0",
    "typescript": "^5.6.3",
    "tsx": "^4.19.2",
    "vite": "^5.4.10",
    "wait-on": "^7.2.0"
  }
}
